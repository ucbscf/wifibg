<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>edu.berkeley.stat.updatesharedwifi</string>
	<key>ProgramArguments</key>
	<array>
	  <string>/var/tmp/update-shared-network-pass.sh</string>
	</array>
        <key>RunAtLoad</key>
        <true/>
        <key>StartInterval</key>
        <integer>300</integer>
	<key>StandardErrorPath</key>
	<string>/dev/null</string>
	<key>StandardOutPath</key>
	<string>/var/log/updatesharedwifi/updatesharedwifi.log</string>
</dict>
</plist>
