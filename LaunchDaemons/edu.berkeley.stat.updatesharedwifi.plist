<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>edu.berkeley.stat.updatesharedwifi</string>
	<key>ProgramArguments</key>
	<array>
	  <string>/usr/local/sbin/update-shared-network-pass.sh</string>
	</array>
        <key>RunAtLoad</key>
        <false/>
        <key>StartCalendarInterval</key>
        <array>
            <dict>
                <key>Weekday</key>
                <integer>7</integer>
                <key>Hour</key>
                <integer>0</integer>
                <key>Minute</key>
                <integer>5</integer>
            </dict>
        </array>
	<key>StandardErrorPath</key>
	<string>/dev/null</string>
	<key>StandardOutPath</key>
	<string>/var/log/wifibg/wifibg.log</string>
</dict>
</plist>
